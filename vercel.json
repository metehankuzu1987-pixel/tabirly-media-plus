{
  "version": 2,
  "functions": { "api/proxy/*.js": { "runtime": "edge" } },
  "rewrites": [
    { "source": "/proxy/ai", "destination": "/api/proxy/ai.js" },
    { "source": "/proxy/stock", "destination": "/api/proxy/stock.js" }
  ],
  "headers": [
    {
      "source": "/proxy/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
